{
  "openapi": "3.1.0",
  "info": {
    "title": "获取模型列表接口",
    "description": "获取所有可用AI模型的列表信息",
    "license": {
      "name": "MIT"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.evolink.ai",
      "description": "生产环境"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "paths": {
    "/v1/models": {
      "get": {
        "summary": "获取模型列表",
        "description": "获取所有可用AI模型的列表信息",
        "operationId": "getModels",
        "tags": ["任务管理"],
        "responses": {
          "200": {
            "description": "模型列表获取成功",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModelListResponse"
                }
              }
            }
          },
          "401": {
            "description": "认证失败",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "429": {
            "description": "请求频率过高",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          },
          "500": {
            "description": "服务器内部错误",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ModelListResponse": {
        "type": "object",
        "required": ["object", "data"],
        "properties": {
          "object": {
            "type": "string",
            "enum": ["list"],
            "description": "对象类型"
          },
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ModelInfo"
            },
            "description": "模型列表"
          }
        }
      },
      "ModelInfo": {
        "type": "object",
        "required": ["id", "object", "created", "owned_by"],
        "properties": {
          "id": {
            "type": "string",
            "description": "模型标识符",
            "example": "gpt-4o"
          },
          "object": {
            "type": "string",
            "enum": ["model"],
            "description": "对象类型"
          },
          "created": {
            "type": "integer",
            "description": "创建时间戳",
            "example": 1626777600
          },
          "owned_by": {
            "type": "string",
            "description": "模型提供商",
            "example": "openai"
          }
        }
      },
      "Error": {
        "type": "object",
        "required": ["error"],
        "properties": {
          "error": {
            "type": "object",
            "required": ["message", "type"],
            "properties": {
              "message": {
                "type": "string",
                "description": "错误消息"
              },
              "type": {
                "type": "string",
                "description": "错误类型"
              },
              "param": {
                "type": "string",
                "nullable": true,
                "description": "相关参数"
              }
            }
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "description": "使用Bearer Token进行认证，格式：Authorization: Bearer YOUR_API_KEY"
      }
    }
  },
  "tags": [
    {
      "name": "任务管理",
      "description": "异步任务管理相关接口"
    }
  ]
}